        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.market_backend.Mapper.ProductTypeMapper">

    <resultMap id="BaseResultMap" type="org.example.market_backend.Entity.ProductType">
